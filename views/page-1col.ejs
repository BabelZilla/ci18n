<html>
	<head>
		<title><%= pageTitle %></title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="/js/ie/html5shiv.js"></script><![endif]-->
		<%- header() %>
		<!--[if lte IE 9]><link rel="stylesheet" href="/css/ie9.css" /><![endif]-->
	</head>
	<body>
		<div id="page-wrapper">
			<div id="header-wrapper">
				<div class="container">
					<div class="row">
						<div class="12u">

							<header id="header">
								<h1><a href="/" id="logo"><%= config.siteName %></a></h1>
								<%- include('primary-nav') %>
							</header>

						</div>
					</div>
				</div>
			</div>
			<div id="main">
				<div class="container">
					<% if(locals.layoutedContent) { %>
						<%- include(locals.layoutedContent) %>
					<% } %>
					<% if(locals.content) { %>
					<div class="row main-row">
						<div class="12u">

							<section>
								<%- include(content) %>
							</section>

						</div>
					</div>
					<% } %>
				</div>
			</div>
			<div id="footer-wrapper">
				<div class="container">
					<div class="row">
						<div class="12u">

							<div id="copyright">
								&copy; ci18n.com - All rights reserved.</a>
							</div>

						</div>
					</div>
				</div>
			</div>
		</div>

		<!-- Scripts -->
			<!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
			<%- footer() %>

	</body>
</html>